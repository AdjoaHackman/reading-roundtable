<section>
    <h1>Reading Roundtables</h1>
    <h2>Perusing Libraries</h2>
    {{#each bookData as |search| }}
    <div class="row align-center mb-5">
        <div class="col-md-6">
            <h3> {{search.title}}</h3>
            <img src={{search.thumbnail}}>
            <p>{{search.authors}}</p>
            <p>{{search.description}}</p>
            <button>Add to My Library</button>
        </div>
    </div>
    {{/each}}
</section>
<script src="/js/search.js"></script>