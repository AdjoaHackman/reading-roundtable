<section>
    <h1>Reading Roundtables</h1>
    <h2>{{userData.name}}'s Library</h2>
    {{#each bookData as |savedBook| }}
    <div class="row align-center mb-5">
        <div class="col-md-6">
            <h3> {{savedBook.title}}</h3>
            <img src={{savedBook.thumbnail}}>
            <p>{{savedBook.authors}}</p>
            <p>{{savedBook.description}}</p>
        </div>
    </div>
    {{/each}}
</section>